<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Mimoto Test Suite">
	<!-- Passing Group as parmeter unsing "testType" Possible Groups are: smoke 
		regression smokeAndRegression -->
	<listeners>
		<listener class-name="io.mosip.report.CustomTestNGReporter" />
		<listener
			class-name="io.mosip.authentication.fw.util.AuthenticationTestsListener" />
		<listener class-name="io.mosip.authentication.e2e.E2EReport" />
		<listener class-name="io.mosip.customReport.EmailableReport" />
	</listeners>
	
	
	
	<test name="AddIdentity">
		<parameter name="ymlFile"
			value="mimoto/AddIdentity/AddIdentity.yml" />
		<classes>
			<class name="io.mosip.testscripts.AddIdentity" />
		</classes>
	</test>
	<test name="GenerateVID">
		<parameter name="ymlFile"
			value="mimoto/GenerateVID/createGenerateVID.yml" />
			<parameter name="idKeyName"	value="vid" />
		<classes>
			<class name="io.mosip.testscripts.PostWithAutogenIdWithOtpGenerate" />
		</classes>
	</test>
	
	 <test name="AuthLock">
		<parameter name="ymlFile"
			value="mimoto/AuthLock/createAuthLock.yml" />
		<classes>
			<class name="io.mosip.testscripts.PostWithBodyWithOtpGenerate" />
		</classes>
	</test>
	<test name="AuthUnLock">
		<parameter name="ymlFile"
			value="mimoto/AuthUnLock/createAuthUnLock.yml" />
		<classes>
			<class name="io.mosip.testscripts.PostWithBodyWithOtpGenerate" />
		</classes>
	</test>
	
	<test name="RequestCredentials">
		<parameter name="ymlFile"
			value="mimoto/CredentialIssuance/RequestCredentials/ReqCredential.yml" />
			<parameter name="idKeyName"	value="requestId" />
		<classes>
			<class name="io.mosip.testscripts.PostWithAutogenIdWithOtpGenerate" />
		</classes>
	</test>
	 <test name="CredentialCheckStatus">
		<parameter name="ymlFile"
			value="mimoto/CredentialIssuance/CredentialCheckStatus/CredentialCheckStatus.yml" />
		<classes>
			<class name="io.mosip.testscripts.GetWithParam" />
		</classes>
	</test>
	
	<test name="DownloadWithReqid">
		<parameter name="ymlFile"
			value="mimoto/CredentialIssuance/DownloadWithReqid/DownloadWithReqid.yml" />
		<classes>
			<class name="io.mosip.testscripts.GetWithParamForDownloadCard" />
		</classes>
	</test>
	
	<test name="SendOTPUIN">
		<parameter name="ymlFile"
			value="mimoto/SendOTPUIN/createSendOTP.yml" />
		<classes>
			<class name="io.mosip.testscripts.SimplePost" />
		</classes>
	</test>
	
	<test name="AidGetIndividualId">
		<parameter name="ymlFile"
			value="mimoto/AidGetIndividualId/AidGetIndividualId.yml" />
		<classes>
			<class name="io.mosip.testscripts.SimplePost" />
		</classes>
	</test>
	
</suite>